<%= form_for [@corporate, @booking], html: { class: 'form-horizontal' } do |f|  %>
	<div class="row">
		<div class="col-lg-6">
			<div class="form-group" id="data_1">
				<label class="col-sm-4 control-label">Pickup Date</label>
		    <div class="col-sm-8">
		    	<div class="input-group date">
		        <span class="input-group-addon">
		        <i class="fa fa-calendar"></i>
		        </span>
		        <input type="text" class="form-control" value="<%= Time.now.strftime("%m/%d/%Y") %>">
		      </div>
		    </div>
		  </div>
	  </div>
	  <div class="col-lg-4">
		  <div class="form-group">
				<label class="col-sm-2 control-label">Time</label>
		    <div class="col-sm-5">		    
		        <input type="text" class="form-control" value="00:00">		   
		    </div>
		  </div>
		</div>
  </div>
  <div class="hr-line-dashed"></div>
  <div class="row">
	  <div class="col-lg-6">
		  <div class="form-group">
				<label class="col-sm-4 control-label">Car Type</label>
		    <div class="col-sm-6">	
	    		<select class="select2_demo_1 form-control select2-hidden-accessible" tabindex="-1" aria-hidden="true">        
            <% @car_types.each do |car_type| %>
            	<option value="#{car_type.id}"><%= car_type.name %></option>
            <% end %>
        	</select>		        	   
		    </div>
		  </div>
		</div>
		<div class="col-lg-6">
		  <div class="form-group">
				<label class="col-sm-4 control-label">Payment Type</label>
		    <div class="col-sm-6">	
	    		<select class="select2_demo_1 form-control select2-hidden-accessible" tabindex="-1" aria-hidden="true">        
            <% @payment_types.each do |payment_type| %>
            	<option value="#{payment_type.id}"><%= payment_type.name %></option>
            <% end %>
        	</select>		        	   
		    </div>
		  </div>
		</div>
	</div>
	<div class="row">
		<div class="col-lg-6">
			<div class="form-group">
				<label class="col-sm-4 control-label">Passenger's Name</label>
		    <div class="col-sm-8">		    
		        <input type="text" class="form-control" placeholder="Passenger's Name">		   
		    </div>
		  </div>
		</div>
		<div class="col-lg-6">
			<div class="form-group">
				<label class="col-sm-4 control-label">Telephone No.</label>
		    <div class="col-sm-8">		    
		        <input type="text" class="form-control" placeholder="Contact Number">		   
		    </div>
		  </div>
		</div>
	</div>
	<div class="hr-line-dashed"></div>
	<div class="row">
		<div class="col-lg-6">
			<div class="form-group">
				<label class="col-sm-4 control-label">From</label>
		    <div class="col-sm-8">		    
		        <input type="text" id="geocompleteFrom" class="form-control" placeholder="Enter From Location">		   
		    </div>
		  </div>
		</div>
		<div class="col-lg-6">
			<div class="form-group">
				<label class="col-sm-4 control-label">To</label>
		    <div class="col-sm-8">		    
		        <input type="text" id="geocompleteTo" class="form-control geocomplete" placeholder="Enter To Location">		   
		    </div>
		  </div>
		</div>
	</div>

  <script type="text/javascript">
  	$(document).ready(function(){
  		$('#data_1 .input-group.date').datepicker({
          todayBtn: "linked",
          keyboardNavigation: false,
          forceParse: false,       
          autoclose: true
      });

      $(".select2_demo_1").select2();

      $("#geocompleteFrom").geocomplete({     
        	details: ".field",
            types: ["geocode", "establishment"],
            componentRestrictions: {country: "ae"},
        });

      $("#geocompleteTo").geocomplete({     
        	details: ".field",
            types: ["geocode", "establishment"],
            componentRestrictions: {country: "ae"},
        });

  	})
  </script>

<% end %>